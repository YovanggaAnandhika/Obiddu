                                    <ImageView
                                        android:id="@+id/imageView6"
                                        android:layout_width="10dp"
                                        android:layout_height="10dp"
                                        app:srcCompat="@drawable/star1"
                                        tools:ignore="VectorDrawableCompat" />
                                </LinearLayout>
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
